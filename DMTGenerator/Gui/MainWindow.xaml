            <MenuItem Header="Файл" OpacityMask="Black">
                <MenuItem Header="Новый" FontSize="12"></MenuItem>
                <MenuItem Header="Открыть" FontSize="12"></MenuItem>
                <MenuItem Header="Сохранить" FontSize="12"></MenuItem>
                <MenuItem Header="Сохранить как..." FontSize="12"></MenuItem>
                <Separator></Separator>
                <MenuItem Header="Выход" Click="MenuItem_Click" FontSize="12"></MenuItem>
            </MenuItem>
        </Menu>
