                        <ColumnDefinition Width="195*"/>
                        <ColumnDefinition Width="256*"/>
                    </Grid.ColumnDefinitions>
                    <StackPanel HorizontalAlignment="Left" Height="63" Margin="40,57,0,0" VerticalAlignment="Top" Width="226" OpacityMask="Black" Grid.ColumnSpan="2">
